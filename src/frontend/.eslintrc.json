{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "max-len": [
      "error",
      {
        "code": 150
      }
    ]
  }
}